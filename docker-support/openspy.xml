<applications>
        <application name="qr">
                <drivers>
                        <driver name="qrserver">
                                <addresses>
                                        <address>
                                                <ip>0.0.0.0</ip>
                                                <port>30694</port>
                                        </address>
                                </addresses>
                        </driver>
                </drivers>
        </application>
        <application name="natneg">
                <drivers>
                        <driver name="nn1">
                                <addresses>
                                        <address>
                                                <ip>0.0.0.0</ip>
                                                <port>30695</port>
                                        </address>
                                </addresses>
                        </driver>
                        <driver name="nn2">
                                <addresses>
                                        <address>
                                                <ip>0.0.0.0</ip>
                                                <port>30696</port>
                                        </address>
                                </addresses>
                        </driver>
                        <driver name="nn3">
                                <addresses>
                                        <address>
                                                <ip>0.0.0.0</ip>
                                                <port>30697</port>
                                        </address>
                                </addresses>
                        </driver>
                </drivers>
        </application>
        <application name="serverbrowsing">
                <drivers>
                        <driver name="sbserver">
                                <addresses>
                                        <address>
                                                <ip>0.0.0.0</ip>
                                                <port>30692</port>
                                        </address>
                                </addresses>
                                <variables>
                                        <variable type="literal" name="protocol-version">1</variable>
                                </variables>
                        </driver>
                        <driver name="sbserver_enctypex">
                                <addresses>
                                        <address>
                                                <ip>0.0.0.0</ip>
                                                <port>30693</port>
                                        </address>
                                </addresses>
                                <variables>
                                        <variable type="literal" name="protocol-version">2</variable>
                                </variables>
                        </driver>
                </drivers>
        </application>
        <application name="GP">
                <drivers>
                        <driver name="gpserver">
                                <addresses>
                                        <address>
                                                <ip>0.0.0.0</ip>
                                                <port>30974</port>
                                        </address>
                                </addresses>
                        </driver>
                </drivers>
        </application>
        <application name="GS">
                <drivers>
                        <driver name="gsserver">
                                <addresses>
                                        <address>
